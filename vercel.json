{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "regions": ["iad1"],
  "functions": {
    "api.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
